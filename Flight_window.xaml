        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Control_Tower_WPF"
        mc:Ignorable="d"
        Title="Flight_window" Height="411.978" Width="595.209" ResizeMode="NoResize" Closing="Flight_Window_Closing">

    <Grid x:Name="Flight_form" Margin="0,0,3.6,-19.6">
        <Grid.ColumnDefinitions>





