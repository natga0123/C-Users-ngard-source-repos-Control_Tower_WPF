        <Button x:Name="Btn_Land" Content="Land" HorizontalAlignment="Left" Margin="425,324,0,0" Width="117" FontSize="20" FontWeight="Bold" RenderTransformOrigin="0.075,-1.954" Height="29" VerticalAlignment="Top" Click="Btn_Land_Click"/>
        <ComboBox x:Name="Combo_Headings" HorizontalAlignment="Left" Margin="216,325,0,0" VerticalAlignment="Top" Width="171" Height="28" RenderTransformOrigin="0.521,1.066" SelectionChanged="Combo_Heading_Sel_Changed"/>

    </Grid>
</Window>
